{
  "name": "keepass-ng",
  "version": "0.0.1",
  "description": "The next generation KeePass",
  "main": "dist/app/index.js",
  "scripts": {
    "build": "babel -d dist/ src/ --source-maps inline --copy-files --quiet",
    "prestart": "npm run build",
    "start": "electron .",
    "hint": "jshint --reporter=node_modules/jshint-stylish --config .jshintrc src/**/*.js",
    "pretest": "npm run hint",
    "test:mocha": "mocha --compilers js:babel-core/register --require src/spec/plainMocha/setup.js src/spec/plainMocha/**/*-Spec.js",
    "test:phantomjs": "karma start",
    "test": "npm run test:mocha && npm run test:phantomjs",
    "watch:test": "nodemon -e js,html,css --watch src/ --exec 'npm run test --silent'",
    "watch:build": "nodemon -e js,html,css --watch src/ --exec 'npm run build --silent'"
  },
  "dependencies": {
    "crypto-js": "^3.1.5",
    "dustjs-linkedin": "~2.6.2",
    "electron-prebuilt": "^0.35.0",
    "fs-extra": "^0.20.1",
    "jade": "^1.11.0",
    "keepass.io": "^1.1.2",
    "loglevel": "^1.4.0",
    "nconf": "^0.8.2",
    "text": "github:requirejs/text"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-syntax-async-functions": "^6.1.18",
    "babel-plugin-transform-es2015-modules-amd": "^6.1.18",
    "babel-plugin-transform-regenerator": "^6.3.2",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "chai-dom": "^1.4.0",
    "es": "^0.4.8",
    "glob": "^6.0.1",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jscs": "^2.6.0",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.1.0",
    "karma": "^0.13.15",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserify": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chai-dom": "^1.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^0.2.1",
    "karma-cli": "^0.1.1",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-requirejs": "^0.2.2",
    "karma-sinon-chai": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "parallelshell": "^2.0.0",
    "proxyquire": "^1.7.3",
    "proxyquire-universal": "^1.0.8",
    "proxyquireify": "^3.0.0",
    "rename": "^1.0.3",
    "run-sequence": "^1.0.1",
    "should": "^7.1.1",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "through2": "^2.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "vinyl-transform": "^1.0.0",
    "watch": "^0.16.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": true
}
